<!-- Modal -->
<div id="changePasswordModal" class="fixed inset-0 flex items-center justify-center bg-columbia-blue bg-opacity-50 hidden">
    <div class="bg-white p-6 rounded-lg shadow-lg relative">
        <h2 class="text-lg font-semibold mb-4">Change Password</h2>
        <form>
            <label for="oldPassword" class="block mb-2">Old Password</label>
            <input type="password" id="oldPassword" class="border rounded p-2 mb-4 w-full">
            <label for="newPassword" class="block mb-2">New Password</label>
            <input type="password" id="newPassword" class="border rounded p-2 mb-4 w-full">
            <label for="confirmPassword" class="block mb-2">Confirm New Password</label>
            <input type="password" id="confirmPassword" class="border rounded p-2 mb-4 w-full">
            <button type="submit" class="bg-columbia-blue hover:bg-gray-700 text-white py-2 px-4 rounded">Save</button>
        </form>
        <button id="closeModal" class="absolute top-2 right-2  columbia-blue   text-gray-500 text-xl">&times;</button>
    </div>
</div>

<script>

document.addEventListener('DOMContentLoaded', () => {
            const openModalButton = document.getElementById('openModal');
            const closeModalButton = document.getElementById('closeModal');
            const modal = document.getElementById('changePasswordModal');

            // Open the modal
            openModalButton.addEventListener('click', (event) => {
                event.preventDefault();
                modal.classList.remove('hidden');
            });

            // Close the modal
            closeModalButton.addEventListener('click', () => {
                modal.classList.add('hidden');
            });

            // Close the modal when clicking outside of it
            window.addEventListener('click', (event) => {
                if (event.target === modal) {
                    modal.classList.add('hidden');
                }
            });
        });




  document.addEventListener('DOMContentLoaded', function () {
    const toggle = document.getElementById('theme-toggle');

    // Check the initial state of dark mode based on user preference or previous session
    if (localStorage.getItem('theme') === 'dark') {
      document.documentElement.classList.add('dark');
      toggle.checked = true;
    }

    // Add event listener to the toggle switch
    toggle.addEventListener('change', function () {
      if (toggle.checked) {
        document.documentElement.classList.add('dark');
        localStorage.setItem('theme', 'dark'); // Save the user's preference
      } else {
        document.documentElement.classList.remove('dark');
        localStorage.setItem('theme', 'light'); // Save the user's preference
      }
    });
  });
</script>
</body>
